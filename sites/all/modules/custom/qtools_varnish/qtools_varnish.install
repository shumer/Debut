<?php
/**
 * @file
 * Install hooks.
 */

/**
 * Implements hook_enable().
 */
function qtools_esi_enable() {

  // Site private key.
  if (!variable_get('qtools_esi_private_key', FALSE)) {
    variable_set('qtools_esi_private_key', uniqid());
  }

}

/**
 * Implements hook_disable().
 */
function qtools_esi_disable() {
}
